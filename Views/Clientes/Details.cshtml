@model Projeto_Integrador_Senac.Models.Cliente

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <h4>Cliente</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente_Nome)
        </dd>
        <dt class = "col-sm-2">
            Sobrenome
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente_Sobrenome)
        </dd>
        <dt class = "col-sm-2">
            Data de Nascimento
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente_Dt_Nasc)
        </dd>
        <dt class = "col-sm-2">
            Celular
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente_Celular)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cliente_Email)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Cliente_ID">Editar</a> |
    <a asp-action="Index">Voltar para a lista</a>
</div>
